{
  "name": "trickeyone/epoch",
  "description": "A general-purpose library to parse, validate, manipulate, and format dates",
  "authors": [
    {
      "name": "Josh England",
      "email": "joshua.en@gmail.com"
    }
  ],
  "keywords": [
    "date",
    "time",
    "parse",
    "format",
    "validate",
    "epoch"
  ],
  "license": "MIT",
  "config": {
    "sort-packages": true,
    "process-timeout": 0,
    "lock": false
  },
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6 || ^10.4"
  },
  "autoload": {
    "psr-4": {
      "Epoch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Epoch\\Test\\": "tests/"
    }
  }
}
